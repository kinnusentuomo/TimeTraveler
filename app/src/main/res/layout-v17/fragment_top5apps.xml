<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:tag="top5tag"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/backgroundstripesleft">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <android.support.v4.widget.Space
                android:layout_weight="0.1"
                android:layout_height="25sp"
                android:layout_width="match_parent"
                />
        </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent" >


        <TextView
            android:id="@+id/textViewTotalTime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/totaltimeusage"
            android:layout_column="1"
            style="@style/TextViewTheme"
            />
    </TableRow>

    <TableRow>
        <Spinner
            style="@style/SpinnerTheme"
            android:id="@+id/spinnerQuerySelect"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="1"
            android:layout_gravity="start"

            />
    </TableRow>


    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/top5apps_label"
            android:textAlignment="center"
            android:layout_column="1"
            style="@style/TextViewTheme"
            />

    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        >

        <ImageView
            android:id="@+id/imageViewTop1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@mipmap/ic_launcher_round"
            android:layout_gravity="center|left"
            />

        <TextView
            android:id="@+id/top1App"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:scrollbars="vertical"
            android:text="Tähän tulee top1 sovelluksen tiedot"
            style="@style/TextViewTheme"
            android:layout_gravity="center|left|left"
            />
        



    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1">

        <ImageView
            android:id="@+id/imageViewTop2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@mipmap/ic_launcher_round"
            android:layout_gravity="center|left"
            />

        <TextView
            android:id="@+id/top2App"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Tähän tulee top2 sovelluksen tiedot"
            style="@style/TextViewTheme"
            android:layout_gravity="center|left"
            />

    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        >

        <ImageView
            android:id="@+id/imageViewTop3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@mipmap/ic_launcher_round"
            android:layout_gravity="center|left"
            />

        <TextView
            android:id="@+id/top3App"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Tähän tulee top3 sovelluksen tiedot"
            style="@style/TextViewTheme"
            android:layout_gravity="center|left"
            />

    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        >

        <ImageView
            android:id="@+id/imageViewTop4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@mipmap/ic_launcher_round"
            android:layout_gravity="center|left"
            />

        <TextView
            android:id="@+id/top4App"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Tähän tulee top4 sovelluksen tiedot"
            style="@style/TextViewTheme"
            android:layout_gravity="center|left"
            />

    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        >

        <ImageView
            android:id="@+id/imageViewTop5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@mipmap/ic_launcher_round"
            android:layout_gravity="center|left"
            />

        <TextView
            android:id="@+id/top5App"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Tähän tulee top5 sovelluksen tiedot"
            style="@style/TextViewTheme"
            android:layout_gravity="center|left"
            />

    </TableRow>
</TableLayout>